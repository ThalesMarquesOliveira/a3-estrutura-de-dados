package estrutura;
    
// Contrato mínimo para uma fila dinâmica.
public interface Fila<E> {
   void enfileirar(E e);       // Insere no fim da fila. 
    
   E desinfileirar();             // Remove e retorna do início da fila; retorna null se vazia. 

   boolean isEmpty();       // Retorna true se não houver elementos. 

   int Tamanho();              // Quantos elementos há na fila. 

   void clear();            // Limpa todos os elementos. 
}
